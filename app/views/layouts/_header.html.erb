  <div class="results">
    <% unless user_signed_in? %>
      <div class="boxfont1 boxheading boxborder ">
        <h1>Log-in</h1>
      </div>
      <div style="margin-top: 10px; margin-left: 10px;">
        <div>
          <%= form_for :user, :url => '/users/sign_in' do |f| %>
            <div style="width:100%;">
              <span>User Name</span>
              <span><%= f.email_field 'email'%></span>
            </div>
            <div>
              <span>Password</span>
              <span><%= f.password_field 'password'%></span>
            </div>
            <div>
              <span style='float:left;'>
                <%= f.submit 'Log In', :style => 'width:90px;' %>
              </span>
              <span style="float: right; margin: 25px 15px 0px 0px;"><a href="/users/auth/facebook" style="float: right; margin: -25px 15px 0px 0px;"><img alt="Fb_login" src="/assets/fb_login.png" /></a></span>
            </div>
          <% end %>
        </div>
        <div style="width:100%;float:left;">
          <span align="center" style="padding-top:7px;float:left;"><a href="/users/sign_up">Register</a></span>
          <span align="center" style="padding:7px 15px 0px 0px;float:right;"><a href="/users/password/new">Forgot Password</a></span>
        </div>
      </div>
    <% else %>
      <%= link_to "Log out", destroy_user_session_path, :method => :delete, :style => 'border:none;' %>
    <% end %>
  </div>
