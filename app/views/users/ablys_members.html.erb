<div class="page-header">
  <h1> ABLYS Members </h1>
</div>
<table class="table table-bordered table-hover">
  <thead>
    <tr width="100%" align="left">
      <th width="25%">Name </th>
      <th width="25%">Father Name </th>
      <th width="25%">Contact Number</th>
      <th width="25%">City</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user_path(user) %></td>
        <td><%= user.f_name %></td>
        <td><%= user.phone %></td>
        <td><%= user.city %></td>
      </tr>
    <% end %>
  </tbody>
</table>